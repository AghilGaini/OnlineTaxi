    <link rel="stylesheet" href="~/css/map.css" />
    

</head>
<body class="container-fluid">
    <div class="text-center mapButtons">
        <input type="button" class="btn btn-success" onclick="getDistanceFromLatLonInKm(firstlatlng.lat,firstlatlng.lng,secondlatlng.lat,secondlatlng.lng)" value="محاسبه مسافت" />
        <input type="button" class="btn btn-primary" onclick="clearMarkers()" value="پاک کردن" />
    </div>
    <div>
        <div class="col-lg-8 offset-lg-2 col-md-8 offset-md-2 col-sm-8 offset-sm-2 mainMap" id="map"></div>
    </div>
</body>
</html>

<script src="~/js/map.js"></script>



